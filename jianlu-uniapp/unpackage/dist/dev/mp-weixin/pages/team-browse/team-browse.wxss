
.browse-container.data-v-ca2390ce {
		padding: 20rpx;
		min-height: 100vh;
		background-color: #f5f5f5;
}
.header.data-v-ca2390ce {
		margin-bottom: 30rpx;
		padding: 20rpx;
		background: #ffffff;
		border-radius: 16rpx;
}
.page-title.data-v-ca2390ce {
		font-size: 32rpx;
		font-weight: bold;
		color: #333;
		display: block;
}
.page-subtitle.data-v-ca2390ce {
		font-size: 24rpx;
		color: #666;
		display: block;
		margin-top: 8rpx;
}
.search-section.data-v-ca2390ce {
		margin-bottom: 30rpx;
}
.search-box.data-v-ca2390ce {
		display: flex;
		align-items: center;
		background: #ffffff;
		border-radius: 16rpx;
		padding: 20rpx;
		gap: 20rpx;
}
.search-input.data-v-ca2390ce {
		flex: 1;
		background: #f8f8f8;
		border-radius: 8rpx;
		padding: 15rpx;
		font-size: 28rpx;
}
.search-btn.data-v-ca2390ce {
		background: #007aff;
		color: #ffffff;
		border: none;
		border-radius: 8rpx;
		padding: 15rpx 30rpx;
		font-size: 28rpx;
}
.teams-list.data-v-ca2390ce {
		height: calc(100vh - 300rpx);
}
.team-item.data-v-ca2390ce {
		margin-bottom: 20rpx;
		padding: 24rpx;
}
.team-header.data-v-ca2390ce {
		display: flex;
		align-items: flex-start;
		margin-bottom: 16rpx;
}
.team-avatar.data-v-ca2390ce {
		width: 80rpx;
		height: 80rpx;
		border-radius: 40rpx;
		background: #007aff;
		display: flex;
		align-items: center;
		justify-content: center;
		color: #ffffff;
		font-size: 32rpx;
		font-weight: bold;
		margin-right: 20rpx;
}
.team-info.data-v-ca2390ce {
		flex: 1;
}
.team-name.data-v-ca2390ce {
		font-size: 32rpx;
		font-weight: bold;
		color: #333;
		display: block;
		margin-bottom: 8rpx;
}
.team-meta.data-v-ca2390ce {
		display: flex;
		flex-direction: column;
		gap: 4rpx;
}
.member-count.data-v-ca2390ce, .leader-name.data-v-ca2390ce {
		font-size: 24rpx;
		color: #666;
}
.team-status.data-v-ca2390ce {
		margin-left: 20rpx;
}
.status-badge.data-v-ca2390ce {
		font-size: 20rpx;
		padding: 6rpx 12rpx;
		border-radius: 16rpx;
		color: #ffffff;
}
.status-none.data-v-ca2390ce {
		background: #34c759;
}
.status-pending.data-v-ca2390ce {
		background: #ff9500;
}
.status-approved.data-v-ca2390ce {
		background: #007aff;
}
.status-rejected.data-v-ca2390ce {
		background: #ff3b30;
}
.status-member.data-v-ca2390ce {
		background: #666;
}
.team-description.data-v-ca2390ce {
		font-size: 28rpx;
		color: #666;
		line-height: 1.5;
		margin-bottom: 16rpx;
}
.team-footer.data-v-ca2390ce {
		display: flex;
		justify-content: space-between;
		align-items: center;
		padding-top: 16rpx;
		border-top: 2rpx solid #f0f0f0;
}
.create-date.data-v-ca2390ce {
		font-size: 24rpx;
		color: #999;
}
.btn.small.data-v-ca2390ce {
		padding: 12rpx 24rpx;
		font-size: 24rpx;
		height: auto;
		line-height: 1.2;
}
.loading-state.data-v-ca2390ce, .empty-state.data-v-ca2390ce {
		text-align: center;
		padding: 60rpx;
		color: #666;
}
.empty-icon.data-v-ca2390ce {
		font-size: 120rpx;
		display: block;
		margin-bottom: 30rpx;
}
.empty-title.data-v-ca2390ce {
		font-size: 36rpx;
		font-weight: bold;
		color: #333;
		display: block;
		margin-bottom: 16rpx;
}
.empty-subtitle.data-v-ca2390ce {
		font-size: 28rpx;
		color: #666;
		display: block;
}
	
	/* 弹窗样式 */
.modal-overlay.data-v-ca2390ce {
		position: fixed;
		top: 0;
		left: 0;
		right: 0;
		bottom: 0;
		background: rgba(0, 0, 0, 0.5);
		display: flex;
		align-items: center;
		justify-content: center;
		z-index: 1000;
}
.modal-content.data-v-ca2390ce {
		width: 90%;
		max-height: 80%;
		background: #ffffff;
		border-radius: 16rpx;
		display: flex;
		flex-direction: column;
}
.modal-header.data-v-ca2390ce {
		display: flex;
		justify-content: space-between;
		align-items: center;
		padding: 30rpx;
		border-bottom: 2rpx solid #f0f0f0;
}
.modal-title.data-v-ca2390ce {
		font-size: 32rpx;
		font-weight: bold;
}
.modal-close.data-v-ca2390ce {
		font-size: 40rpx;
		color: #999;
}
.modal-body.data-v-ca2390ce {
		flex: 1;
		padding: 30rpx;
		overflow-y: auto;
}
.form-item.data-v-ca2390ce {
		margin-bottom: 30rpx;
}
.label.data-v-ca2390ce {
		display: block;
		font-size: 28rpx;
		color: #333;
		margin-bottom: 12rpx;
}
.textarea.data-v-ca2390ce {
		width: 100%;
		height: 120rpx;
		padding: 20rpx;
		border: 2rpx solid #e0e0e0;
		border-radius: 8rpx;
		font-size: 28rpx;
		box-sizing: border-box;
		resize: none;
}
.modal-footer.data-v-ca2390ce {
		display: flex;
		gap: 20rpx;
		padding: 30rpx;
		border-top: 2rpx solid #f0f0f0;
}
.modal-footer .btn.data-v-ca2390ce {
		flex: 1;
		height: 80rpx;
		line-height: 80rpx;
		text-align: center;
		border-radius: 8rpx;
		font-size: 28rpx;
}
